<div class="adminActionHead">
	<div class="row">
		<div class="col-md-6">
			<span class="title" id="mainTitle"><%= modelTitle || modelName %>: Index</span>
			<div class="clear"></div>
			<div class="actions paginate-actions">
				<% chimera.actions('paginate') %>
			</div>
		</div>
		<div class="col-md-2"></div>
		<div class="col-md-4">
			<% if (!useSeparateFilterPage) {
				print_element('chimera/model_editor_filters');
			} else {
				print_element('chimera/model_editor_filters_modal');
			} %>
		</div>
	</div>
</div>