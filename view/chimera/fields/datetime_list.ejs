<% start('field') %>
<%
	dateval = data.value == null ? '' : data.value;

	if (dateval) {
		print('<time datetime="' + dateval.toISOString() + '">');
		print(dateval.toDateString() + ' - ');
		print(dateval.getHours(true).toPaddedString(2) + ':' + dateval.getMinutes().toPaddedString(2));
		print('</time>');
	}
%>
<% end('field') %>